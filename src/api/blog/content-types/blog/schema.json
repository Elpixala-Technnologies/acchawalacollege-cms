{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blog_content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "blog_title": {
      "type": "string",
      "required": true
    },
    "blog_url": {
      "type": "string",
      "required": true
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "excerpt": {
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "is_top": {
      "type": "boolean",
      "default": false
    },
    "colleges": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::college.college",
      "mappedBy": "blog"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "blog"
    },
    "exams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::exam.exam",
      "mappedBy": "blog"
    },
    "scholarships": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::scholarship.scholarship",
      "mappedBy": "blog"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "blog"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "blogs"
    }
  }
}
