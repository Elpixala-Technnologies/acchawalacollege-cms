{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_url": {
      "type": "uid",
      "required": true
    },
    "course_name": {
      "type": "string",
      "required": true
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialization.specialization",
      "mappedBy": "courses"
    },
    "page_data": {
      "type": "dynamiczone",
      "components": [
        "common.gallery",
        "common.new-overview",
        "common.faq",
        "common.recommended-college",
        "common.banner-component",
        "common.recommended-courses",
        "common.recommended-exams"
      ]
    },
    "is_top": {
      "type": "boolean",
      "default": false
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "pin_code": {
      "type": "integer"
    },
    "streams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stream.stream",
      "inversedBy": "courses"
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "courses"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "common.seo"
    },
    "review_component": {
      "displayName": "review_component",
      "type": "component",
      "repeatable": false,
      "component": "common.review-component",
      "required": true
    },
    "college_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::colleges-type.colleges-type",
      "mappedBy": "course"
    },
    "approvedBy": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organisation.organisation",
      "mappedBy": "courses"
    },
    "user_forms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-form.user-form",
      "mappedBy": "course"
    },
    "courseLevels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-level.course-level",
      "inversedBy": "courses"
    },
    "navbars": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::navbar.navbar",
      "inversedBy": "courses"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "courses"
    },
  },
  "news": {
    "type": "relation",
    "relation": "manyToMany",
    "target": "api::new.new",
    "inversedBy": "courses",
  },
    "bg": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "eligibility": {
      "type": "text"
    },
    "fees": {
      "type": "biginteger"
    },
    "course_duration": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course-duration.course-duration",
      "inversedBy": "course"
    },
    "buttons": {
      "type": "component",
      "repeatable": true,
      "component": "common.button"
    }
  }
}
