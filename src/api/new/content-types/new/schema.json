{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "news",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "required": true
    },
    "excerpt": {
      "type": "text",
      "maxLength": 125
    },
    "featuredImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "blog",
        "news"
      ],
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "common.seo"
    },
    "scholarships": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::scholarship.scholarship",
      "mappedBy": "news"
    },
    "colleges": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::college.college",
      "mappedBy": "news"
    },
    "exams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::exam.exam",
      "mappedBy": "news"
    },
    "news_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::news-category.news-category",
      "mappedBy": "news"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "news"
    }
  }
}
